{
	"name": "html-to-hs-extension",
	"displayName": "html-to-hs-extension",
	"description": "Extension for converting html to hyperscript",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:html-to-hs-extension.toReactHyperscript",
		"onCommand:html-to-hs-extension.toHyperscript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "html-to-hs-extension.toReactHyperscript",
				"title": "Convert to React Hyperscript"
			},
			{
				"command": "html-to-hs-extension.toHyperscript",
				"title": "Convert to Hyperscript"
			}
		],
		"menus": {
			"editor/context": [{
					"when": "resourceExtname == .svg",
					"command": "html-to-hs-extension.toReactHyperscript",
					"group": "navigation"
			}, {
				"when": "resourceExtname == .svg",
				"command": "html-to-hs-extension.toHyperscript",
				"group": "navigation"
		}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.49.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"html-to-hyperscript": "^0.8.0"
	}
}
